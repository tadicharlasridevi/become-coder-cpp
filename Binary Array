#include<iostream>
#include<vector>
using namespace std;
int main()
{
    int n,c=0,a;
    cin>>n;
    vector<int>v;
    for(int i=0;i<n;i++)
    {
        cin>>a;
        v.emplace_back(a);
    }
    for(int i=0;i<n;i++)
    {
        if(v[i]==1 || v[i]==0)
        c=c+1;
    }
    if(c==n)
    cout<<"True";
    else
    cout<<"False";
}
