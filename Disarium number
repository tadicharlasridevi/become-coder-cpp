
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,s=0;
    cin>>n;
    int e=n;
    int d=trunc(log10(n)+1);
    while(d!=0)
    {
        int r=n%10;
        s=s+pow(r,d);
        n=n/10;
        d--;
    }
    if(e==s)
    cout<<"True";
    else
    cout<<"False";
}



n=175
1**1+ 7**2 + 5**3 = 1+ 49 + 125 = 175

