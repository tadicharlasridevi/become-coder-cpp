#include<iostream>
#include<vector>
using namespace std;
int main()
{
    int n,a,sum=0;
    cin>>n;
    vector<int>v;
    for(int i=0;i<n;i++)
    {
        cin>>a;
        v.emplace_back(a);
    }
    for(int i=0;i<n;i++)
    {
        if(v[i]%2==1)
        {
            sum=sum+v[i];
        }
    }
    cout<<sum;
}
