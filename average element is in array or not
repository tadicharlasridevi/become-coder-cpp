#include<iostream>
using namespace std;
int main()
{
    int n,sum=0,flag=0;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    for(int i=0;i<n;i++)
    {
        sum=sum+a[i];
    }
   int avg=sum/n;
    for(int i=0;i<n;i++)
    {
        if(avg==a[i])
        {
            flag++;
            break;
        }
    }
    if(flag==1)
    cout<<"True";
    else
    cout<<"False";
    
}
